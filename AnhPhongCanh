package DAKhoAnhPhongCanh;

public class AnhPhongCanh{

    private String Name, TacGia, KichThuoc;
    private NgayNhapXuat date;

    public AnhPhongCanh() {
    }

    public AnhPhongCanh(String Name, String TacGia, String KichThuoc, NgayNhapXuat date) {
        this.Name = Name;
        this.TacGia = TacGia;
         this.KichThuoc = KichThuoc;
        this.date = date;
    }

    public String getName() {
        return Name;
    }

    public void setName(String Name) {
        this.Name = Name;
    }

    public String getTacGia() {
        return TacGia;
    }

    public void setTacGia(String TacGia) {
        this.TacGia = TacGia;
    }


    public String getKichThuoc() {
        return KichThuoc;
    }

    public void setKichThuoc(String KichThuoc) {
        this.KichThuoc = KichThuoc;
    }
     public NgayNhapXuat getDate() {
        return date;
    }

    public void setDate(NgayNhapXuat date) {
        this.date = date;
    }

    @Override
    public String toString() {
        return "Name=" + Name + ", TacGia=" + TacGia + ", KichThuoc=" + KichThuoc+ ", date=" + date;
    }
}
